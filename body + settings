<!DOCTYPE html>
<html lang="fr">
	<head>
		<meta charset="utf-8" />
		<title>Body Smooth Wheel</title>
		<link rel="icon" href="data:;base64,iVBORw0KGgo=">
		<style>
			*,*:before,*:after{
				box-sizing:border-box;
				margin:0;
				padding:0;
				outline:none;
				scrollbar-color:rgba(0,0,0,0.5) rgba(255,255,255,0.3);
			}
			html{
				width:100%;
				height:100%;
			}
			body{
				position:relative;
				width:100%;
				height:100%;
				overflow:auto;
				padding:3vw;
				color:#000;
			}
			div{
				width:200px;
				height:200px;
				overflow:auto;
			}
			.interface{
				position:fixed;
				top:10px;
				right:10px;
				width:auto;
				height:auto;
				padding:20px;
				border:1px solid #000;
				background-color:#fff;

			}
			.interface input{
				display:block;
				margin-top:0px;
			}
			.interface p{
				
				margin-top:20px;
			}
		</style>
		<script>
				var intFocal=50;
				var intSpeed=20;
			if(localStorage.getItem('Focal')){
				intFocal=parseInt(localStorage.getItem('Focal'));
				intSpeed=parseInt(localStorage.getItem('Speed'));
			} 
			
			 smooth=(e)=>{
				
				e['nbScroll']=[];
				e['inScroll']=false;
				e['goScroll']=()=>{
					if(e.scrollTop<e.scrollHeight) {
						let intLastValue=e.nbScroll[e.nbScroll.length-1];
						let intPas=Math.round(intSpeed*(intLastValue/(intFocal+1)));
						(e.nodeName.toLowerCase()=='body')?
							window.scrollTo(0,document.documentElement.scrollTop+(-intPas))
						:
							e.scrollTop+=-intPas;
						intLastValue+=intLastValue<0?1:-1;
						e.nbScroll[e.nbScroll.length-1]=intLastValue;
						if(intLastValue!=0)
							setTimeout(()=>{e.goScroll()},(intFocal+1)-Math.abs(intLastValue));
						else {
							e.nbScroll=[];
							e.inScroll=false;
						}
					}
				}
				e['doSmooth']=()=>{
					if(e.inScroll)
						return 0;
					else {
						if(e.nbScroll.length>0) {
							e.inScroll=true;
							e.goScroll();
						} else
							e.inScroll=false;
					}
				}
				e.addEventListener('wheel',(ev)=>{
					ev.preventDefault();
					ev.stopPropagation();
					e.nbScroll.push(ev.wheelDeltaY>0?intFocal:-intFocal);
					e.doSmooth();
				},{passive:0});
			}
			window.onload=()=>{
				smooth(document.body);

				var resuFocal = document.querySelector('#counterFocal');
				var resuSpeed = document.querySelector('#counterSpeed');
	
					resuFocal.textContent = intFocal;
					resuSpeed.textContent = intSpeed;
				
				document.querySelector('#rangFocal').value = intFocal;
				document.querySelector('#rangFocal').addEventListener('input', (event) => {
					
					resuFocal.textContent = event.target.value;
					intFocal = Number(event.target.value);	
					localStorage.setItem('Focal', intFocal);
				
				});


				document.querySelector('#rangSpeed').value = intSpeed;
				document.querySelector('#rangSpeed').addEventListener('input', (event) => {
					
					resuSpeed.textContent = event.target.value;
					intSpeed = Number(event.target.value);
					localStorage.setItem('Speed', intSpeed);
			
				});
			}

			
	
		</script>
	</head>
	<body bgcolor="#FFF">
		<div class="interface">
			<h5>Scroll settings:</h5>

				<p id="counterFocal">50</p>
				<input type="range" id="rangFocal" name="Focal" 
				min="0" max="100" value="50" step="1">
				<label for="volume">Focal</label>
			

				<p id="counterSpeed">20</p>
				<input type="range" id="rangSpeed" name="Speed" 
						min="0" max="100" value="20" step="1">
				<label for="cowbell">Speed</label>
			
		</div>
